<?xml version="1.0" encoding="utf-8"?>
 
<AbsoluteLayout 
	android:id="@+id/AbsoluteLayout01"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:minHeight="1200px"
	>

	<!-- Row 1 -->
	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/white"
		android:padding="@dimen/imageview_padding"
		android:layout_x="25px"
		android:layout_y="45px">

		<ImageView
		    android:id="@+id/stuinfo"
		    android:layout_width="@dimen/imageview_size"
		    android:layout_height="@dimen/imageview_size"
		    android:scaleType="matrix"
		    android:src="@drawable/stuinfo" >

		</ImageView>
	</LinearLayout>

	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/white"
		android:padding="@dimen/imageview_padding"
		android:layout_x="225px"
		android:layout_y="45px">

		<ImageView
		    android:id="@+id/material"
		    android:layout_width="@dimen/imageview_size"
		    android:layout_height="@dimen/imageview_size"
		    android:scaleType="matrix"
		    android:src="@drawable/h2_material"  >

		</ImageView>
	</LinearLayout>	

	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/white"
		android:padding="@dimen/imageview_padding"
		android:layout_x="420px"
		android:layout_y="45px">

		<ImageView
		    android:id="@+id/reading"
		    android:layout_width="@dimen/imageview_size"
		    android:layout_height="@dimen/imageview_size"
		    android:scaleType="matrix"
		    android:src="@drawable/h2_reading" >

		</ImageView>
	</LinearLayout>
	
	

	<!-- Row 2 -->

	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/white"
		android:padding="@dimen/imageview_padding"
		android:layout_x="25px"
		android:layout_y="230px">

		<ImageView
		    android:id="@+id/myrefer"
		    android:layout_width="@dimen/imageview_size"
		    android:layout_height="@dimen/imageview_size"
		    android:scaleType="matrix"
		    android:src="@drawable/h2_refer" >

		</ImageView>
		
	</LinearLayout>

	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/white"
		android:padding="@dimen/imageview_padding"
		android:layout_x="225px"
		android:layout_y="230px">
		<ImageView
			 android:id="@+id/mydoc"
			android:src="@drawable/h2_mydoc"
			android:layout_height="@dimen/imageview_size"
			android:layout_width="@dimen/imageview_size"
			android:scaleType="matrix">
		</ImageView>
	</LinearLayout>

	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/white"
		android:padding="@dimen/imageview_padding"
		android:layout_x="420px"
		android:layout_y="230px">
		<ImageView
			android:id="@+id/appTool"
			android:src="@drawable/h2_apptool"
			android:layout_height="@dimen/imageview_size"
			android:layout_width="@dimen/imageview_size"
			android:scaleType="matrix">
		</ImageView>
	</LinearLayout>


	<!-- Row 3 -->

	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/white"
		android:padding="@dimen/imageview_padding"
		android:layout_x="25px"
		android:layout_y="415px">
		<ImageView
			 android:id="@+id/search"
			android:src="@drawable/h2_search"
			android:layout_height="@dimen/imageview_size"
			android:layout_width="@dimen/imageview_size"
			android:scaleType="matrix">
		</ImageView>
	</LinearLayout>

	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/white"
		android:padding="@dimen/imageview_padding"
		android:layout_x="225px"
		android:layout_y="415px">
		<ImageView
			android:id="@+id/imgComm"
			android:src="@drawable/h2_comm"
			android:layout_height="@dimen/imageview_size"
			android:layout_width="@dimen/imageview_size"
			android:scaleType="matrix">
		</ImageView>
	</LinearLayout>


	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/white"
		android:padding="@dimen/imageview_padding"
		android:layout_x="420px"
		android:layout_y="415px">
		<ImageView
			android:id="@+id/webinternet"
			android:src="@drawable/h2_webinternet"
			android:layout_height="@dimen/imageview_size"
			android:layout_width="@dimen/imageview_size"
			android:scaleType="matrix">
		</ImageView>
	</LinearLayout>

	

	<!-- Row 4 -->

	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/white"
		android:padding="@dimen/imageview_padding"
		android:layout_x="25px"
		android:layout_y="600px">
		<ImageView
			android:id="@+id/apks"
			android:src="@drawable/h2_apks"
			android:layout_height="@dimen/imageview_size"
			android:layout_width="@dimen/imageview_size"
			android:scaleType="matrix">
		</ImageView>
	</LinearLayout>

	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/white"
		android:padding="@dimen/imageview_padding"
		android:layout_x="225px"
		android:layout_y="600px">
		<ImageView
			android:id="@+id/info"
			android:src="@drawable/h2_info"
			android:layout_height="@dimen/imageview_size"
			android:layout_width="@dimen/imageview_size"
			android:scaleType="matrix">
		</ImageView>
	</LinearLayout>

	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/white"
		android:padding="@dimen/imageview_padding"
		android:layout_x="420px"
		android:layout_y="600px">

		<ImageView
		    android:id="@+id/syssetting"
		    android:layout_width="@dimen/imageview_size"
		    android:layout_height="@dimen/imageview_size"
		    android:scaleType="fitXY"
		    android:src="@drawable/h2_syssetting"  >

		</ImageView>
	</LinearLayout>

	
	
	<TextView
	    android:id="@+id/textName"
	    android:layout_width="560dp"
	    android:layout_height="wrap_content"
	    android:layout_x="25dp"
	    android:layout_y="10dp"
	    android:text="." />

</AbsoluteLayout>
